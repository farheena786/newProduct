{
  "name": "dtc-portal-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "hostBuild": "ng serve --host 0.0.0.0 --disable-host-check",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "jest",
    "install:local": "npm install ../../../lib/msal-angular/dist ../../../lib/msal-browser",
    "install:published": "npm install @azure/msal-angular@latest @azure/msal-browser@latest",
    "angular-cli": "npm install @angular/cli@14.2.0",
    "app-start": "npm run angular-cli && npm install && npm start",
    "app-build": "npm run angular-cli && npm install && npm run build",
    "code-coverage": "ng test --no-watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
